+#!/usr/bin/env bash
+# Helper script to push the current branch to a GitHub remote.
+# Usage:
+#   ./scripts/push-to-github.sh --repo git@github.com:your-org/your-repo.git [--branch main] [--remote origin]
+# Environment variable fallback:
+#   REPO_URL (if --repo not provided)
+#   REMOTE_NAME (default: origin)
+#   BRANCH (default: current checked-out branch)
+set -euo pipefail
+
+usage() {
+  cat <<'MSG'
+Usage: ./scripts/push-to-github.sh --repo <git-url> [--branch <branch>] [--remote <name>]
+Options:
+  --repo, -r    Git remote URL (falls back to REPO_URL env)
+  --branch, -b  Branch to push (defaults to current branch)
+  --remote      Remote name to use/create (default: origin or REMOTE_NAME env)
+MSG
+}
+
+REMOTE_NAME="${REMOTE_NAME:-origin}"
+BRANCH="${BRANCH:-}"
+REPO_URL="${REPO_URL:-}"
+
+while [[ $# -gt 0 ]]; do
+  case "$1" in
+    --repo|-r)
+      REPO_URL="${2:-}"
+      shift 2
+      ;;
+    --branch|-b)
+      BRANCH="${2:-}"
+      shift 2
+      ;;
+    --remote)
+      REMOTE_NAME="${2:-}"
+      shift 2
+      ;;
+    -h|--help)
+      usage
+      exit 0
+      ;;
+    *)
+      echo "Unknown option: $1" >&2
+      usage >&2
+      exit 1
+      ;;
+  esac
+done
+
+if ! git rev-parse --is-inside-work-tree >/dev/null 2>&1; then
+  echo "Error: must be run inside a git repository." >&2
+  exit 1
+fi
+
+CURRENT_BRANCH=$(git rev-parse --abbrev-ref HEAD)
+BRANCH="${BRANCH:-$CURRENT_BRANCH}"
+
+if [[ -z "${REPO_URL}" ]] && ! git remote get-url "${REMOTE_NAME}" >/dev/null 2>&1; then
+  cat >&2 <<'MSG'
+Error: no remote configured and REPO_URL is missing.
+Set REPO_URL to your GitHub repository URL, e.g.:
+  REPO_URL=git@github.com:your-org/your-repo.git ./scripts/push-to-github.sh
+MSG
+  exit 1
+fi
+
+if ! git remote get-url "${REMOTE_NAME}" >/dev/null 2>&1; then
+  git remote add "${REMOTE_NAME}" "${REPO_URL}"
+fi
+
+git push -u "${REMOTE_NAME}" "${BRANCH}"
